<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="sale_order_form_line_tree_inherit_warehouse" model="ir.ui.view">
        <field name="name">sale.order.order.line.tree.temporary.override</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='order_line']//tree" position="inside">
                <field name="product_warehouse_id"/>
                <field name="warehouse_qty_available" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>
